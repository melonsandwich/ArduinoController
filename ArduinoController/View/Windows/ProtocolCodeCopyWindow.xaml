<Window x:Class="ArduinoController.View.Windows.ProtocolCodeCopyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ArduinoController.View.Windows"
        mc:Ignorable="d"
        Title="Код протокола" Height="450" Width="800" WindowStartupLocation="CenterScreen" WindowStyle="ToolWindow">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBox Name="TextBoxProtocolCode" Height="300" Width="500" FontFamily="Consolas" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto"/>
            <Button Name="ButtonCopyProtocolCode" Content="_Скопировать" Click="ButtonCopyProtocolCode_Click"/>
        </StackPanel>
    </Grid>
</Window>
